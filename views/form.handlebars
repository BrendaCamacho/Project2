            <div class="row">
                <div class="col s12">
                    <div class="row">
                        <h1>Add a Post...</h1>
                        <div class="input-field col s12">
                            <i class="material-icons prefix">account_circle</i>
                            <input type="text" name="bandName" id="bandName"></input>
                            <label for="bandName">Band Name</label>
                        </div>
                    </div>
                    <select name="bandLocation" id="bandLocation">
                        <option value="Guadalajara">Guadalajara</option>
                        <option value="Monterrey">Monterrey</option>
                        <option value="Mexico City">Mexico City</option>
                    </select>
                    <label for="bandLocation">Choose location</label>
                </div>
                <div class="input-field col s12">
                    <select name="bandVacancy"  id="bandVacancy">
                        <option value="Guitarist">Guitarist</option>
                        <option value="Vocalist">Vocalist</option>
                        <option value="Drums">Drums</option>
                        <option value="Synth">Synth</option>
                    </select>
                    <label for="bandVacancy">Choose what you are looking for</label>
                </div>
        
                <div class="input-field col s12">
                    <select  id="genre">
                        <option value="Rock">Rock</option>
                        <option value="Pop">Pop</option>
                        <option value="Jazz">Jazz</option>
                    </select>
                    <label>Choose band's genre</label>
                </div>
                <div class="input-field col s12">
                    <input id="bandDescription" type="text" data-length="10" name="bandDescription">
                    <label for="bandDescription">Describe your band</label>
                </div>
                <div class="input-field col s12">
                        <input id="bandFblink" type="text" data-length="10"  name="bandFbLink">
                        <label for="bandFblink">Add a link to your Facebook</label>
                    </div>
                <button class="btn waves-effect waves-light right" id="submitBand" type="submit" name="action">POST
                    <i class="material-icons right">send</i>
                </button>
            </div>
            <br><br><br><br><br><br>
            <!-- empieza foooter -->
            <div class="row">
